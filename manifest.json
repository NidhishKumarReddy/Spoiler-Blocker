{
  "manifest_version": 2,

  "name": "Hide Avengers Spoilers",
  "description": "Trying to save you from Avengers Spoiler.",
  "version": "2.0",
  "icons": { 
          "96": "AvengersLogo.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.4.0.min.js", "spoiler.js"],
      "run_at": "document_end"
    }
  ]
}
